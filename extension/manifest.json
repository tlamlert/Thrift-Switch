{
    "manifest_version": 2,
    "name": "Swiggies extention",
    "description": "Sustainable alternative of clothes",
    "version": "1.1",
    "permissions": [
      "storage",
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches":["*://*/*"],
        "js":["content.js"]
        }
    ]
}
