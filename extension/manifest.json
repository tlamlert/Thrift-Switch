{
    "manifest_version": 3,
    "name": "Swiggies extention",
    "description": "Sustainable alternative of clothes",
    "version": "1.1",
    "permissions": [
      "storage",
      "scripting",
      "tabs",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches":["*://*/*"],
        "js":["content.js"]
        }
    ]
}
