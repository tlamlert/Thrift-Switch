{
    "manifest_version": 3,
    "name": "Swiggies extention",
    "description": "Sustainable alternative of clothes",
    "version": "1.1",
    "permissions": ["storage", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches":["*://*/*"],
        "js":["img.js"]
    }
    ]
}